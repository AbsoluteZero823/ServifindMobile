{
  "expo": {
    "name": "ServiFind",
    "slug": "servifind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drickz.servifind"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f0f0f0"
      },
      "package": "com.drickz.servifind"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "drickz",
    "extra": {
      "eas": {
        "projectId": "148c2961-7d78-4e1d-9bc7-0da77e814c6e"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/148c2961-7d78-4e1d-9bc7-0da77e814c6e"
    }
  }
}
